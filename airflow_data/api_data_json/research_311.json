d-itemidlist": {"itemid": {"$": "33746571099", "@idtype": "SGR"}}, "ref-volisspag": {"voliss": {"@volume": "22", "@issue": "3"}, "pagerange": {"@first": "360"}}, "ref-text": "COI: 1:CAS:528:DC%2BD28XlvFCksb4%3D", "ref-authors": {"author": [{"@seq": "1", "ce:initials": "Q.", "@_fa": "true", "ce:surname": "Zhang", "ce:indexed-name": "Zhang Q."}, {"@seq": "2", "ce:initials": "B.", "@_fa": "true", "ce:surname": "Kang", "ce:indexed-name": "Kang B."}, {"@seq": "3", "ce:initials": "H.", "@_fa": "true", "ce:surname": "Xu", "ce:indexed-name": "Xu H."}, {"@seq": "4", "ce:initials": "H.", "@_fa": "true", "ce:surname": "Lin", "ce:indexed-name": "Lin H."}]}, "ref-sourcetitle": "Chemical Research in Chinese Universities"}}, {"ref-fulltext": "R. Kohen and A. Nyska, Toxicologic Pathology, 30(6), 620 (2002).", "@id": "28", "ref-info": {"ref-publicationyear": {"@first": "2002"}, "refd-itemidlist": {"itemid": {"$": "0036867895", "@idtype": "SGR"}}, "ref-volisspag": {"voliss": {"@volume": "30", "@issue": "6"}, "pagerange": {"@first": "620"}}, "ref-text": "COI: 1:CAS:528:DC%2BD38XovVCqtLo%3D", "ref-authors": {"author": [{"@seq": "1", "ce:initials": "R.", "@_fa": "true", "ce:surname": "Kohen", "ce:indexed-name": "Kohen R."}, {"@seq": "2", "ce:initials": "A.", "@_fa": "true", "ce:surname": "Nyska", "ce:indexed-name": "Nyska A."}]}, "ref-sourcetitle": "Toxicologic Pathology"}}, {"ref-fulltext": "G. Stephanopoulos, Chemical process control: An introduction to theory and Practices, Prentice Hall (1984).", "@id": "29", "ref-info": {"ref-publicationyear": {"@first": "1984"}, "refd-itemidlist": {"itemid": {"$": "0004104880", "@idtype": "SGR"}}, "ref-authors": {"author": [{"@seq": "1", "ce:initials": "G.", "@_fa": "true", "ce:surname": "Stephanopoulos", "ce:indexed-name": "Stephanopoulos G."}]}, "ref-sourcetitle": "Chemical process control: An introduction to theory and Practices, Prentice Hall"}}]}}}}, "affiliation": {"affiliation-city": "Bangkok", "@id": "60028190", "affilname": "Chulalongkorn University", "@href": "https://api.elsevier.com/content/affiliation/affiliation_id/60028190", "affiliation-country": "Thailand"}, "coredata": {"srctype": "j", "eid": "2-s2.0-84964067312", "dc:description": "Due to the presence of growth inhibitor and high impurity concentration in biodiesel wastewater, both biologicaln and chemical processes are ineffective for treating such wastewater. In this work, biodiesel wastewater was treated by electrooxidation via Ti/RuO2 electrodes in batch and continuous modes. Effects of the additive type, hydrogen peroxide (H2O2) and sodium chloride (NaCl), and concentration on the treatment efficiency, monitored in terms of the reduction in the biological oxygen demand (BOD), chemical oxygen demand (COD) and oil and grease level, were explored. The addition of NaCl gave higher treatment efficiency than H2O2, and both were higher than no addition, due to the continuous generation of the oxidizing chloride species. The removal of almost all the COD and oil and grease and ~95% BOD was obtained in the presence of 0.061 M NaCl at an applied current density of 4.28 mA/cm2 for 7h. In continuous operation mode, the steady state condition was reached within 11 h and the treatment efficiency decreased as the wastewater feed rate increased. By using wastewater feed rate of 2mL/min, approximately 83.56, 61.43 and 91.72% of BOD, COD and oil and grease levels were respectively removed. The rate of pollutant removal fitted a first order reaction for both the batch and continuous operation modes.", "prism:coverDate": "2016-07-01", "prism:aggregationType": "Journal", "prism:url": "https://api.elsevier.com/content/abstract/scopus_id/84964067312", "dc:creator": {"author": [{"ce:given-name": "Pattaraluk", "preferred-name": {"ce:given-name": "Pattaraluk", "ce:initials": "P.", "ce:surname": "Jaruwat", "ce:indexed-name": "Jaruwat P."}, "@seq": "1", "ce:initials": "P.", "@_fa": "true", "affiliation": {"@id": "60028190", "@href": "https://api.elsevier.com/content/affiliation/affiliation_id/60028190"}, "ce:surname": "Jaruwat", "@auid": "35186160500", "author-url": "https://api.elsevier.com/content/author/author_id/35186160500", "ce:indexed-name": "Jaruwat P."}]}, "link": [{"@_fa": "true", "@rel": "self", "@href": "https://api.elsevier.com/content/abstract/scopus_id/84964067312"}, {"@_fa": "true", "@rel": "scopus", "@href": "https://www.scopus.com/inward/record.uri?partnerID=HzOxMe3b&scp=84964067312&origin=inward"}, {"@_fa": "true", "@rel": "scopus-citedby", "@href": "https://www.scopus.com/inward/citedby.uri?partnerID=HzOxMe3b&scp=84964067312&origin=inward"}], "source-id": "13609", "citedby-count": "6", "prism:volume": "33", "subtype": "ar", "dc:title": "Treatment of biodiesel wastewater by indirect electrooxidation: Effect of additives and process kinetics", "openaccess": "0", "prism:issn": "02561115", "publishercopyright": "\u00a9 2016, Korean Institute of Chemical Engineers, Seoul, Korea.", "prism:issueIdentifier": "7", "subtypeDescription": "Article", "prism:publicationName": "Korean Journal of Chemical Engineering", "prism:pageRange": "2090-2096", "prism:endingPage": "2096", "openaccessFlag": "false", "prism:doi": "10.1007/s11814-016-0045-2", "prism:startingPage": "2090", "dc:identifier": "SCOPUS_ID:84964067312", "dc:publisher": "Springer New York LLCbarbara.b.bertram@gsk.com"}, "idxterms": null, "language": {"@xml:lang": "eng"}, "authkeywords": {"author-keyword": [{"@_fa": "true", "$": "Biodiesel Wastewater"}, {"@_fa": "true", "$": "Indirect Electrooxidation"}, {"@_fa": "true", "$": "NaCl"}]}, "subject-areas": {"subject-area": [{"@_fa": "true", "$": "Chemistry (all)", "@code": "1600", "@abbrev": "CHEM"}, {"@_fa": "true", "$": "Chemical Engineering (all)", "@code": "1500", "@abbrev": "CENG"}]}, "authors": {"author": [{"ce:given-name": "Pattaraluk", "preferred-name": {"ce:given-name": "Pattaraluk", "ce:initials": "P.", "ce:surname": "Jaruwat", "ce:indexed-name": "Jaruwat P."}, "@seq": "1", "ce:initials": "P.", "@_fa"